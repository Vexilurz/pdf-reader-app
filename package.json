{
  "name": "electron-react-typescript-boilerplate",
  "version": "2.0.0",
  "description": "Template project for desktop apps using Electron, React and Typescript",
  "author": "Marcelo Augusto",
  "license": "MIT",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "hot"
  ],
  "homepage": "https://github.com/marceloaugusto80/electron-react-typescript-boilerplate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marceloaugusto80/electron-react-typescript-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/marceloaugusto80/electron-react-typescript-boilerplate/issues"
  },
  "main": "main-process.js",
  "scripts": {
    "format": "eslint src/**/*.ts{,x} --fix",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "build": "npx webpack --env.NODE_ENV=development --env.target=render && npx webpack --env.NODE_ENV=development --env.target=main",
    "build-prod": "npx webpack --env.NODE_ENV=production --env.target=render && npx webpack --env.NODE_ENV=production --env.target=main",
    "server": "npx webpack-dev-server --env.NODE_ENV=development --env.target=render --hot --progress",
    "start": "npx webpack --env.NODE_ENV=development --env.target=main && npx electron ./dist/.",
    "pack": "npm run build-prod && npx electron-packager ./dist --out ./pack --overwrite"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@material-ui/core": "^4.11.0",
    "@reduxjs/toolkit": "^1.4.0",
    "archiver": "^5.0.2",
    "bootstrap": "^4.5.3",
    "electron-pdf-window": "^1.0.12",
    "electron-print-dialog": "0.1.0-alpha.1",
    "konva": "^7.2.1",
    "luxon": "^1.25.0",
    "pdf-to-printer": "^1.5.2",
    "print-js": "^1.5.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.4.0",
    "react-color": "^2.19.3",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.2",
    "react-konva": "^16.13.0-6",
    "react-measure": "^2.5.2",
    "react-pdf": "^5.0.0",
    "react-redux": "^7.2.1",
    "react-resize-panel": "^0.3.5",
    "react-router-dom": "^5.2.0",
    "react-virtualized": "^9.22.2",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/node": "^14.10.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "electron": "^10.1.2",
    "electron-packager": "^15.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^6.1.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^4.4.1",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.2",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
